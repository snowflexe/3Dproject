        //Setting Seahorse (TIFFANY)
        //###############
        //movement Setting 
        var seahorseHeadX = -6, seahorseHeadY = 5, seahorseHeadZ = -3;
        var seahorseSpeed = 0.008, direction = 1;// 1 for moving right, -1 for moving left
        var seahorseSpeedY = 0.05, directionY = 1;// 1 for moving up, -1 for moving down
        var SeahorseSpeedVar = 1;
        var SeahorseBool = true, SeahorseforwardBool = true;
        var timerSeahorse = setInterval(function(){ seahorseHeadMove() }, 10);
    
        var seahorseTail2Rotate = 0;
        var seahorseTail2RotationSpeed = 0.05; 
        var seahorseTail2Direction = 1;

        var seahorseTail3Rotate = 0;
        var seahorseTail3RotationSpeed = 0.07; 
        var seahorseTail3Direction = 1;      
        
        var seahorseFin1OffsetX = 0;
        var seahorseFin1Speed = 0.02; 
        var seahorseFin1Direction = 1;

        var seahorseFin2OffsetX = 0;
        var seahorseFin2Speed = 0.03; 
        var seahorseFin2Direction = -1;

        function stopSeahorse() {
            if (SeahorseBool == true) { SeahorseBool = false; }
            else { SeahorseBool = true; }

            if (SeahorseBool == true) {
                timerSeahorse = setInterval(function(){ animateTurtleMoveLoop() }, 10);
                document.getElementById("stopSeahorse").value = "Stop Seahorse";
            } else {
                clearInterval(timerSeahorse);
                document.getElementById("stopSeahorse").value = "Start Seahorse";
            }
        }

        function speedSeahorse() {
            SeahorseSpeedVar = document.getElementById("speedSeahorse").value; 
            document.getElementById("SeahorseLabel").innerHTML = SeahorseSpeedVar;
        }
        
        function seahorseHeadMove(){
            // Update the cube's X position
            seahorseHeadX += seahorseSpeed * direction;
            if (seahorseHeadX > 4 || seahorseHeadX < -4) {
                direction *= -1; // Reverse direction
            }

            seahorseHeadY += seahorseSpeedY * directionY;
            if (seahorseHeadY > 6 || seahorseHeadY < 3.5) { // Example boundaries
                directionY *= -1; // Reverse Y direction
            }

            seahorseTail2Rotate += seahorseTail2RotationSpeed * seahorseTail2Direction;
            if (seahorseTail2Rotate > 0.4 || seahorseTail2Rotate < -0.4) { // Rotate within a range (radians)
                seahorseTail2Direction *= -1;
            }

            seahorseTail3Rotate += seahorseTail3RotationSpeed * seahorseTail3Direction;
            if (seahorseTail3Rotate > 0.6 || seahorseTail3Rotate < -0.6) { // Rotate within a range (radians)
                seahorseTail3Direction *= -1;
            }

            seahorseFin1OffsetX += seahorseFin1Speed * seahorseFin1Direction;
            if (seahorseFin1OffsetX > 0.2 || seahorseFin1OffsetX < 0) { // Boundaries for relative X movement
                seahorseFin1Direction *= -1;
            }

            seahorseFin2OffsetX += seahorseFin2Speed * seahorseFin2Direction;
            if (seahorseFin2OffsetX > 0.2 || seahorseFin2OffsetX < 0) { // Boundaries for relative Y movement
               seahorseFin2Direction *= -1;
            }

            main();
        }

        //Setting for colourRGB (pong)
        var shHeadColour = seahorseHeadColour, shColorBool = true;
        function colorSeahorse() { //colorParent2
            if (shColorBool == true) { shColorBool = false; } 
            else { shColorBool = true; }

            alphaParent3(); 
            main();
        }

        var shBodyColor =seahorseBodyColour, shBodyColorBool = true;
        function colorSeahorseBody() { //colorChild2
            if (shBodyColorBool == true) { shBodyColorBool = false; } 
            else { shBodyColorBool = true; }

            if (shBodyColorBool == true) { shBodyColor = seahorseBodyColourr; }
            else { shBodyColor = turtleHandColour; }
            
            main();
        }

        var alphaP3Value = 1;
        function alphaParent3() {
            alphaP3Value = document.getElementById("alphaParent3").value / 10; 
            document.getElementById("aP3Label").innerHTML = parseFloat(alphaP3Value).toFixed(1);;

            if (shColorBool == true) {
                shHeadColor = [ 
                    [1, 0.475, 0.188, alphaP3Value],
                    [1, 0.475, 0.188, alphaP3Value],
                    [1, 0.475, 0.188, alphaP3Value],
                    [1, 0.475, 0.188, alphaP3Value],
                    [1, 0.475, 0.188, alphaP3Value],
                    [1, 0.475, 0.188, alphaP3Value],
                ];
            } else {
                shBodyColor = [ 
                    [0.773, 0.89, 0.98, alphaP2Value],    
                    [0.773, 0.89, 0.98, alphaP2Value],   
                    [0.773, 0.89, 0.98, alphaP2Value],   
                    [0.773, 0.89, 0.98, alphaP2Value],   
                    [0.773, 0.89, 0.98, alphaP2Value],   
                    [0.773, 0.89, 0.98, alphaP2Value],   
                ];
            }

            main();
        } 




         
        

