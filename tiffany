            // ###################################################### //
            //Draw Seahorse -(TIFFANY)
            // Reset modelViewMatrix to draw another object
            seahorseHeadMatrix = mat4.create(); 
            // Drag mouse to rotateX & rotateY
            mat4.rotateX(seahorseHeadMatrix,seahorseHeadMatrix,rotateX);   
            mat4.rotateY(seahorseHeadMatrix,seahorseHeadMatrix,rotateY);  
            mat4.rotateZ(seahorseHeadMatrix,seahorseHeadMatrix,rotateZ);

            //Draw head (main)
            colorsBuffer = initBuffers(gl,positionsBox,seahorseHeadColour);  
            mat4.translate(seahorseHeadMatrix,seahorseHeadMatrix,[seahorseHeadX,seahorseHeadY,seahorseHeadZ]); //coordinate
            mat4.scale(seahorseHeadMatrix,seahorseHeadMatrix,[0.4,0.4,0.4]); //size
            drawScene(gl, programInfo, colorsBuffer, seahorseHeadMatrix, projectionMatrix);
            gl.drawElements(gl.TRIANGLES, 36, gl.UNSIGNED_SHORT, 0);
            
            
            // Draw NOSE (Child of Head)
            colorsBufferSeahorseNose = initBuffers(gl, positionsBox, seahorseBodyColour);
            let seahorseNoseMatrix = mat4.clone(seahorseHeadMatrix); 
            mat4.translate(seahorseNoseMatrix, seahorseNoseMatrix, [0, 0.1, -0.6]); // Position relative to head (slightly up, forward)
            mat4.scale(seahorseNoseMatrix, seahorseNoseMatrix, [0.15, 0.1, 0.8]); // longer, thinner
            drawScene(gl, programInfo, colorsBufferSeahorseNose, seahorseNoseMatrix, projectionMatrix);
            gl.drawElements(gl.TRIANGLES, 36, gl.UNSIGNED_SHORT, 0);
            
            // Draw BODY (Child of Head)
            colorsBufferSeahorseBody = initBuffers(gl, positionsBox, seahorseBodyColour);
            let seahorseBodyMatrix = mat4.clone(seahorseHeadMatrix); 
            mat4.translate(seahorseBodyMatrix, seahorseBodyMatrix, [0, -1.0, 0]); // Position relative to head (downwards)
            mat4.scale(seahorseBodyMatrix, seahorseBodyMatrix, [0.6, 2.0, 0.6]); // taller, slightly wider than head
            drawScene(gl, programInfo, colorsBufferSeahorseBody, seahorseBodyMatrix, projectionMatrix);
            gl.drawElements(gl.TRIANGLES, 36, gl.UNSIGNED_SHORT, 0);

            
            // Draw TAIL Segment 1 (Child of Body)
            colorsBufferSeahorseTail1 = initBuffers(gl, positionsBox, seahorseTailColour);
            let seahorseTail1Matrix = mat4.clone(seahorseBodyMatrix); 
            mat4.translate(seahorseTail1Matrix, seahorseTail1Matrix, [0, -1.2, 0]); // Position relative to body (downwards)
            mat4.scale(seahorseTail1Matrix, seahorseTail1Matrix, [0.6, 0.7, 0.6]); // Tail Segment 1 shape
            drawScene(gl, programInfo, colorsBufferSeahorseTail1, seahorseTail1Matrix, projectionMatrix);
            gl.drawElements(gl.TRIANGLES, 36, gl.UNSIGNED_SHORT, 0);

            // Draw TAIL Segment 2 (Child of Tail Segment 1, Animates)
            colorsBufferSeahorseTail2 = initBuffers(gl, positionsBox, seahorseHeadColour);
            let seahorseTail2Matrix = mat4.clone(seahorseTail1Matrix); 
            mat4.translate(seahorseTail2Matrix, seahorseTail2Matrix, [0, -0.5, 0]); // Position relative to Tail 1's center (downwards)
            mat4.rotateX(seahorseTail2Matrix, seahorseTail2Matrix, seahorseTail2Rotate); // Apply animated rotation
            mat4.scale(seahorseTail2Matrix, seahorseTail2Matrix, [0.7,1.5,0.7]); 
            drawScene(gl, programInfo, colorsBufferSeahorseTail2, seahorseTail2Matrix, projectionMatrix);
            gl.drawElements(gl.TRIANGLES, 36, gl.UNSIGNED_SHORT, 0);
          
            // Draw TAIL Segment 3 (Child of Tail Segment 2, Animates)
            colorsBufferSeahorseTail3 = initBuffers(gl, positionsBox, seahorseTailColour);
            let seahorseTail3Matrix = mat4.clone(seahorseTail2Matrix); 
            mat4.translate(seahorseTail3Matrix, seahorseTail3Matrix, [0, -0.5, 0]); // Position relative to Tail 2's center (downwards)
            mat4.rotateX(seahorseTail3Matrix, seahorseTail3Matrix, seahorseTail3Rotate); 
            mat4.scale(seahorseTail3Matrix, seahorseTail3Matrix, [0.6, 1.5, 0.6]); //  smallest
            drawScene(gl, programInfo, colorsBufferSeahorseTail3, seahorseTail3Matrix, projectionMatrix);
            gl.drawElements(gl.TRIANGLES, 36, gl.UNSIGNED_SHORT, 0);

            // Draw FIN 1 (Child of Head, Animates)
            colorsBufferSeahorseFin1 = initBuffers(gl, positionsBox, seahorseTailColour);
            let seahorseFin1Matrix = mat4.clone(seahorseHeadMatrix); 
            mat4.translate(seahorseFin1Matrix, seahorseFin1Matrix, [-0.5+ seahorseFin1OffsetX,0,1.2]);
            mat4.scale(seahorseFin1Matrix, seahorseFin1Matrix, [0.1, 1, 0.5]); 
            drawScene(gl, programInfo, colorsBufferSeahorseFin1, seahorseFin1Matrix, projectionMatrix);
            gl.drawElements(gl.TRIANGLES, 36, gl.UNSIGNED_SHORT, 0);

            // Draw FIN 2 (Child of Head, Animates)
            colorsBufferSeahorseFin2 = initBuffers(gl, positionsBox, seahorseTailColour);
            let seahorseFin2Matrix = mat4.clone(seahorseHeadMatrix); 
            mat4.translate(seahorseFin2Matrix, seahorseFin2Matrix, [0.5+ seahorseFin2OffsetX, 0,  1.2 ]);
            mat4.scale(seahorseFin2Matrix, seahorseFin2Matrix, [0.1, 1, 0.5]); 
            drawScene(gl, programInfo, colorsBufferSeahorseFin2, seahorseFin2Matrix, projectionMatrix);
            gl.drawElements(gl.TRIANGLES, 36, gl.UNSIGNED_SHORT, 0);


        }

    

